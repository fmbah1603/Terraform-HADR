  backend_pool {
    name             = "traffic-manager-pool"
    load_balancing   = "Weighted"
    backend {
      address       = "${azurerm_traffic_manager_profile.example.fqdn}"
      http_port     = 80
      https_port    = 443
      priority      = 1
      weight        = 100
    }

